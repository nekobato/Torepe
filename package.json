{
  "name": "trace-on",
  "description": "Torepe: Support design implemetation with overlay comparison, like digital tracing paper.",
  "author": {
    "email": "nekobato@gmail.com",
    "name": "nekobato",
    "url": "https://nekobato.net"
  },
  "homepage": "https://github.com/nekobato/torepe",
  "repository": {
    "type": "git",
    "url": "https://github.com/nekobato/torepe"
  },
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node scripts/watch.mjs",
    "prebuild": "NODE_ENV=production vue-tsc --project packages/renderer/tsconfig.json --noEmit && node scripts/build.mjs",
    "build": "NODE_ENV=production electron-builder --config ./electron-builder.config.js",
    "publish": "NODE_ENV=production electron-builder --publish always -c ./electron-builder.config.js",
    "debug": "npm run prebuild && vite ./packages/renderer",
    "docs:dev": "vite dev ./packages/docs",
    "docs:build": "vite build ./packages/docs",
    "electron:generate-icons": "electron-icon-builder --input=./packages/renderer/public/icon.png --output=./build --flatten"
  },
  "main": "dist/main/index.cjs",
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "electron": "28.1.1",
    "electron-builder": "^24.6.4",
    "electron-icon-builder": "^2.0.1",
    "electron-updater": "^6.1.4",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-resolve": "^2.5.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.22"
  },
  "env": {
    "PORT": 3000
  },
  "dependencies": {
    "png-dpi-reader-writer": "^0.2.4"
  }
}
