{
  "name": "torepe",
  "description": "Transparent image window as a tracing paper",
  "author": {
    "email": "nekobato@gmail.com",
    "name": "nekobato",
    "url": "https://nekobato.net"
  },
  "homepage": "https://github.com/nekobato/torepe",
  "repository": {
    "type": "git",
    "url": "https://github.com/nekobato/torepe"
  },
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node scripts/watch.mjs",
    "prebuild": "NODE_ENV=production vue-tsc --project packages/renderer/tsconfig.json --noEmit && node scripts/build.mjs",
    "build": "NODE_ENV=production electron-builder --config ./electron-builder.config.js",
    "debug": "npm run prebuild && vite ./packages/renderer",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "main": "dist/main/index.cjs",
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "electron": "25.3.1",
    "electron-builder": "^24.4.0",
    "electron-icon-builder": "^2.0.1",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-resolve": "^2.3.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.6"
  },
  "env": {
    "PORT": 3000
  },
  "dependencies": {
    "png-dpi-reader-writer": "^0.2.4"
  }
}
